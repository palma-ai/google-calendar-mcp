{
  "name": "google-calendar-mcp",
  "version": "1.2.1",
  "description": "Google Calendar MCP HTTP server with Express and per-request authentication support",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "google-calendar-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "test:server": "node scripts/test-server.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "google-calendar",
    "mcp",
    "cursor",
    "ai",
    "oauth",
    "model-context-protocol",
    "google-calendar",
    "claude",
    "auto-auth",
    "express",
    "http-server"
  ],
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@types/express": "^5.0.2",
    "express": "^5.1.0",
    "google-auth-library": "^9.15.0",
    "googleapis": "^144.0.0",
    "mcp-evals": "^1.0.18",
    "open": "^10.0.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@vitest/coverage-v8": "^3.1.1",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  }
}